@model RESToran.Models.ReservationPeriod

@{
    ViewData["Title"] = "Create";
    Restaurant restaurant = ViewBag.Restaurant;
    string date = ViewBag.Date;
    string startTime = ViewBag.StartTime;
    string endTime = ViewBag.EndStart;
}
<link href="~/css/bad_req.css" rel="stylesheet" type="text/css">

<p class="mt-40 f-s-20">All the tables on date: @date in period: @startTime  from: @endTime are alredy reserved, please try another period.</p>


<div>
    <a asp-action="Create" asp-route-id="@restaurant.Id" class="btn btn-dark float-right mt-3">Try again</a>
</div>